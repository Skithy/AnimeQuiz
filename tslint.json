{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "no-console": false,
    "member-access": [
      true,
      "no-public"
    ]
  }
}
